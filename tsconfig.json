{
    "compilerOptions": {
        "target": "ES2015",
        "module": "Node16",
        "moduleResolution": "Node16",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "skipLibCheck": true,
        "strictPropertyInitialization": false
    },
    "exclude": ["dist"]
}